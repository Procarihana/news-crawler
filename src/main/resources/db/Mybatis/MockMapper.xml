<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.Hana.MockMapper">
    <select id="selectNews" resultType="com.github.Hana.MockNews">
        select title, content, url, created_at,modified_at from NEWS
    </select>

    <insert id="insertNews" parameterType="com.github.Hana.MockNews">
        insert into NEWS(url,title,content,created_at,modified_at)
        values (#{url},#{title},#{content},#{createdAt},#{modifiedAt})
    </insert>
</mapper>
